{
    "name": "tablex",
    "version": 2,
    "builds": [
        {
            "src": "./packages/website/package.json",
            "use": "@now/static-build",
            "config": {
                "includeFiles": "./packages/website/doc/**",
                "distDir": "./.docz/dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/packages/website/((?!index).*)",
            "destination": "/packages/website/index.html"
        }
    ],
    "alias": ["tablex.now.sh"],
    "github": {
        "silent": false
    }
}